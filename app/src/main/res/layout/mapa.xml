<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainMapa">
    <ImageView
            android:layout_width="727px"
            android:layout_height="132dp" app:srcCompat="@mipmap/logocarpooling"
            android:id="@+id/imageView4"
            android:scaleType="centerCrop"
            android:adjustViewBounds="true" android:cropToPadding="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="@string/tiempo"
            android:textColor="@color/colorPrimary"
            android:textSize="40sp"
            android:layout_width="192dp"
            android:layout_height="61dp"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="372dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/imageView4"
            app:layout_constraintEnd_toStartOf="@+id/editText4" android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" app:layout_constraintVertical_bias="0.058"/>
    <TextView
            android:textColor="@color/colorPrimary"
            android:textSize="40sp"
            android:layout_width="192dp"
            android:layout_height="61dp"
            android:id="@+id/editText4"

            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView4" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" app:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_marginEnd="30dp" android:layout_marginRight="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.021"/>
    <Button
            android:text="Cancelar"
            android:layout_width="wrap_content"
            android:layout_height="38dp"
            android:id="@+id/buttonCancelar"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="248dp" android:layout_marginStart="248dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="16dp"
            android:layout_marginTop="318dp" app:layout_constraintTop_toBottomOf="@+id/editText4"
            android:layout_marginEnd="16dp" android:layout_marginRight="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0"/>
    <android.support.constraint.ConstraintLayout
            android:layout_height="500dp"
            android:layout_width="500dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="50dp" android:layout_marginStart="50dp" android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="15dp" app:layout_constraintBottom_toTopOf="@+id/buttonCancelar"
            android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/textView4">

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>